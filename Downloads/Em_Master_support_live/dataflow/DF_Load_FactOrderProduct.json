{
    "name": "DF_Load_FactOrderProduct",
    "properties": {
        "type": "MappingDataFlow",
        "typeProperties": {
            "sources": [
                {
                    "dataset": {
                        "referenceName": "FACT_SALESORDER",
                        "type": "DatasetReference"
                    },
                    "name": "source1"
                }
            ],
            "sinks": [
                {
                    "dataset": {
                        "referenceName": "FACT_ORDERPRODUCT",
                        "type": "DatasetReference"
                    },
                    "name": "sink1"
                }
            ],
            "transformations": [
                {
                    "name": "derivedColumn1"
                }
            ],
            "scriptLines": [
                "source(output(",
                "          dwtc_orderproducttotalamount as decimal(38,2),",
                "          barter as string,",
                "          GlCode as string,",
                "          RequestorType as string,",
                "          Contact_GR_ID as integer,",
                "          dwtc_orderid as string,",
                "          Event_ID as integer,",
                "          CreatedOn as timestamp,",
                "          StatusCode as string",
                "     ),",
                "     allowSchemaDrift: true,",
                "     validateSchema: false,",
                "     isolationLevel: 'READ_UNCOMMITTED',",
                "     query: 'select op.[dwtc_orderproducttotalamount], fact.barter,pg.[GlCode], fact.[RequestorType], fact.[Contact_GR_ID],op.[dwtc_orderid], fact.[Event_ID], fact.[CreatedOn] ,fact.[StatusCode]FROM \\n[EM_DWH].[FACT_SALESORDER] factÂ \\nleft join [ods].[EM_OrderProduct_CRM] op on op.[dwtc_orderid]=fact.[Salesorderid]\\nleft join [ods].[EM_Product_PriceList_CRM] ppl on ppl.[dwtc_pricelistproductid]=op.[dwtc_orderpricelistproduct]\\nleft join (select distinct ProductID,PGID from [ods].[EM_ProductMaster_CRM]) pm on pm.[ProductID]=ppl.[dwtc_product]\\nleft join (select distinct PGID,GlCode from [ods].[EM_ProductGroup_CRM]) pg on pg.[PGID]=pm.[PGID]\\n',",
                "     format: 'query',",
                "     staged: true) ~> source1",
                "source1 derive(Execution_Date = currentUTC()) ~> derivedColumn1",
                "derivedColumn1 sink(allowSchemaDrift: true,",
                "     validateSchema: false,",
                "     input(",
                "          Salesorderid as string,",
                "          Event_ID as integer,",
                "          StatusCode as string,",
                "          CreatedOn as timestamp,",
                "          Amount_OrderProduct as decimal(38,2),",
                "          RequestorType as string,",
                "          GlCode as string,",
                "          Barter as string,",
                "          Execution_Date as timestamp,",
                "          Contact_GR_ID as integer",
                "     ),",
                "     deletable:false,",
                "     insertable:true,",
                "     updateable:false,",
                "     upsertable:false,",
                "     truncate:true,",
                "     format: 'table',",
                "     staged: true,",
                "     allowCopyCommand: true,",
                "     skipDuplicateMapInputs: true,",
                "     skipDuplicateMapOutputs: true,",
                "     errorHandlingOption: 'stopOnFirstError',",
                "     mapColumn(",
                "          Amount_OrderProduct = dwtc_orderproducttotalamount,",
                "          Barter = barter,",
                "          GlCode,",
                "          RequestorType,",
                "          Contact_GR_ID,",
                "          Salesorderid = dwtc_orderid,",
                "          Event_ID,",
                "          CreatedOn,",
                "          StatusCode,",
                "          Execution_Date",
                "     )) ~> sink1"
            ]
        }
    }
}